{
  "name": "durabull-app",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Production-ready image hashing app using Durabull workflow engine",
  "main": "dist/backend/server.js",
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:worker\" \"npm --prefix frontend run dev -- --host\"",
    "dev:backend": "tsx watch backend/server.ts",
    "dev:worker": "tsx watch backend/worker.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "tsc && cd frontend && npm run build",
    "start": "node dist/backend/server.js",
    "start:worker": "node dist/backend/worker.js",
    "lint": "eslint backend --ext .ts",
    "lint:fix": "eslint backend --ext .ts --fix",
    "format": "prettier --write \"backend/**/*.ts\" \"frontend/src/**/*.{ts,tsx}\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "durabull",
    "workflow",
    "fastify",
    "react",
    "redis",
    "bullmq",
    "image-hashing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^8.3.0",
    "@webbuf/blake3": "^3.0.28",
    "bullmq": "^5.63.0",
    "durabull": "^0.1.0",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.2",
    "dotenv": "^16.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "concurrently": "^8.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.0"
  }
}
