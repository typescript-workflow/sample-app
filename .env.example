# Example environment variables for local development
# COPY THIS FILE to `.env` and fill in values. Do NOT commit secrets.

# Redis connection URL (used by Durabull/BullMQ)
REDIS_URL=redis://127.0.0.1:6379

# API Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Frontend: base URL for API requests (used by Vite in development/build)
# Example: VITE_API_BASE=http://localhost:3000
VITE_API_BASE=
